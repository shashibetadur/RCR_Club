<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.rcr.domain.MembershipType" table="MEMBERSHIP_TYPE" catalog="rcr_raichur">
        <id name="id" type="long">
            <column name="ID"/>
            <generator class="identity"/>
        </id>
        <property name="name" type="string">
            <column name="PHONE_TYPE"/>
        </property>
        <property name="validity" type="java.lang.Character">
            <column name="VALIDITY"/>
        </property>
        <property name="enrollmentFees" type="double">
            <column name="ENROLLMENT_FEES"/>
        </property>
        <property name="fees" type="double">
            <column name="ENROLLMENT_FEES"/>
        </property>
        <property name="deleteFlag" type="java.lang.Character">
            <column name="DELETE_FLAG"/>
        </property>
    </class>
</hibernate-mapping>