<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.rcr.domain.MembershipDetail" table="MEMBERSHIP_DETAIL" catalog="rcr_raichur">
        <id name="id" type="long">
            <column name="ID"/>
            <generator class="identity"/>
        </id>
        <property name="memberId" type="long" insert="false" update="false">
            <column name="MEMBER_ID" not-null="true"/>
        </property>
        <many-to-one name="membershipType" class="com.rcr.domain.MembershipType" fetch="select" lazy="false">
            <column name="MEMBERSHIP_TYPE_ID" not-null="true"/>
        </many-to-one>
        <property name="startDate" type="timestamp">
            <column name="START_DATE"/>
        </property>
        <property name="endDate" type="timestamp">
            <column name="END_DATE"/>
        </property>
        <property name="enrollmentFees" type="double">
            <column name="ENROLLMENT_FEES"/>
        </property>
        <property name="fees" type="double">
            <column name="FEES"/>
        </property>
        <property name="deleteFlag" type="java.lang.Character">
            <column name="DELETE_FLAG"/>
        </property>
    </class>
</hibernate-mapping>