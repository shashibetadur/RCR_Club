<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.rcr.domain.MemberVisit" table="MEMBER_VISIT" catalog="rcr_raichur">
        <id name="id" type="long">
            <column name="ID"/>
            <generator class="identity"/>
        </id>
        <property name="memberId" type="long" insert="false" update="false">
            <column name="MEMBER_ID" not-null="true"/>
        </property>
        <property name="guestCount" type="long">
            <column name="GUEST_COUNT"/>
        </property>
        <property name="visitDate" type="timestamp">
            <column name="VISIT_DATE"/>
        </property>
        <property name="deleteFlag" type="java.lang.Character">
            <column name="DELETE_FLAG"/>
        </property>
    </class>
</hibernate-mapping>